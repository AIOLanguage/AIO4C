//성공적인 테스트 (Successful test):
Str getSeason(Int monthNumber) {
    mu season = voi
    whe (monthNumber) {
        (12, 1, 2) -> { season = 'Winter' }
        (3, 4, 5) -> { season = 'Spring' }
        (6, 7, 8) -> { season = 'Summer' }
        (9, 10, 11) -> { season = 'Autumn' }
    }
    out season
}

//성공적인 테스트 (Successful test):
Int getDaysInMonthIn2018(Str month) {
    whe (month) {
        'February' -> { out 28 }
        ('April', 'June', 'September', 'November') -> { out 30 }
        ('January', 'March', 'May', 'July', 'August', 'October', 'December') -> { out 31 }
    } : {
        out voi
    }
}