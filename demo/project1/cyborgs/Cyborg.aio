typ Cyborg -> Person, Robot {

    + model Int

    new(
        + firstName Str
        + secondName Str
        + age Int
        + core Core
        + model Int
    ) : Person(firstName, secondName, age), Robot(core)
    {
        $model = model
    }

    fun calc() : override
    {
        println('Calc temperature...')
        println('Calc distance...')
    }

    fun say(+ message Str) : override
    {
        println(':::: #message ::::')
    }
}