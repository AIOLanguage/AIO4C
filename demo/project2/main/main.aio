plg {
    main.springApplication.RobotApplication
    robots.abstractRobot.Robot
    java.util.logging.Logger
}

+ log Logger : private = Logger.getLogger(Main::class.name)

fun main(+ args[] Str)
{
    + robotApplication = RobotApplication()
    + robot Robot = robotApplication.groovyApplicationContext.getBean('starKRERobot')
    log.info('\n\n')
    robot.calc()
}