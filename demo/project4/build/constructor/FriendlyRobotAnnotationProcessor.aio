class FriendlyRobotAnnotationProcessor <- AIOProcessor
    {
        fun before(o Voi, params Voi...) Voi : override
            {
                val friendlyRobotAnnotation = o.class.annotations[FriendlyRobot::class]
                if (friendlyRobotAnnotation) {
                    println('Found friendly robot')
                }
                out o
            }

        fun after(o Voi, params Voi..., result Voi...) Voi : override = o
    }