class FriendlyRobotAnnotationProcessor <- AIOProcessor
    {
        fun before(clazz AIOClass) AIOClass : override
            {
                val friendlyRobotAnnotation = clazz.annotations[FriendlyRobot::class]
                if (friendlyRobotAnnotation) {
                    println('Found friendly robot')
                }
                out clazz
            }

        fun after(clazz AIOClass) AIOClass : override = clazz
    }